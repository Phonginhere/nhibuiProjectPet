<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="249dp"
        android:layout_height="212dp"
        app:srcCompat="@drawable/service_dog_footer_logo"
        tools:layout_editor_absoluteX="-18dp"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="164dp"
        android:layout_height="59dp"
        android:text="Service"
        android:textColor="@color/blue"
        android:textSize="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.687"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.127" />

    <ImageButton
        android:id="@+id/button9"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_weight="1"
        android:adjustViewBounds="true"
        android:backgroundTint="@color/red"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:src="@drawable/pixlr_bg_result__7_"
        tools:layout_editor_absoluteX="98dp"
        tools:layout_editor_absoluteY="251dp" />

    <ImageButton
        android:id="@+id/button_topleft"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_weight="1"
        android:adjustViewBounds="true"
        android:backgroundTint="@color/red"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:src="@drawable/pixlr_bg_result__4_"
        tools:layout_editor_absoluteX="-2dp"
        tools:layout_editor_absoluteY="251dp" />

    <ImageButton
        android:id="@+id/button8"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:backgroundTint="@color/red"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:src="@drawable/pixlr_bg_result__8_"
        android:textColor="@color/white"
        tools:layout_editor_absoluteX="198dp"
        tools:layout_editor_absoluteY="251dp" />

    <ImageButton
        android:id="@+id/button6"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:backgroundTint="@color/red"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:src="@drawable/pixlr_bg_result__9_"
        android:textColor="@color/white"
        tools:layout_editor_absoluteX="298dp"
        tools:layout_editor_absoluteY="251dp" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavigationView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:menu="@menu/bottom_nav_menu" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="353dp"
        android:layout_height="60dp"
        android:ems="10"
        android:hint="Search"
        android:inputType="textPersonName"
        android:textColorHint="@color/blue"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="377dp" />

    <androidx.recyclerview.widget.RecyclerView
        android:layout_width="409dp"
        android:layout_height="236dp"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="438dp" />

</androidx.constraintlayout.widget.ConstraintLayout>